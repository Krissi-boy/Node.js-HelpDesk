<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="stylesheet" href="user.css">
    <title>EJS</title>
</head>
<body>
    <h1>Logged in as</h1>

    <form action="/user" method="post"></form>
    <ul class="user-list">
      <% data.forEach(function(d) { %>
        <li class="user-item">
          <strong class="brukernavn"></strong><%= d.brukernavn %></strong>
          <strong class="mellomnavn"></strong><%= d.mellomnavn %></strong>
          <strong class="etternavn"></strong><%= d.etternavn %></strong> 
          <strong class="alder"><%= d.alder %></strong>
          <strong class="email"><%= d.email %></strong> 
        </li>
      <% }); %>
    </ul>
    
    <div class="container">
      <div class="header">
        <h1>Welcome to HelpDesk</h1>
      </div>
      
      <div class="nav">
        <ul>
          <li><a href="FAQ_site">FAQ</a></li>
          <li><a href="about_us">About us</a></li>
          <li><a href="contact_agent">Contact Agent</a></li>
          <li><a href="case_status">Status Case</a></li>
          <li><a href="user_info">User Info</a></li>
        </ul>
      </div>
      
      <div class="content">
        <h2>We are here for you...Always</h2>
      </div>
      
      <div class="footer">
        <p>&copy; Kristoffer's HelpDesk Systems</p>
      </div>
    </div>

    <div id="holder">
      Change your mind?
    </div>
        
    <button><a href="/log_out">Logout</a></button>
    <button><a href="/change_account">Change account?</a></button>
 
    <script src="klient.js"></script>
</body>
</html>