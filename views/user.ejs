<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="stylesheet" href="user.css">
    <title>EJS</title>
</head>
<body>
    <h1>HelpDesk</h1>

    <form action="/user" method="post"></form>

    <ul>
        <% data.forEach(function(d) { %>
          <li>
            <strong><%= d.brukernavn %></strong> 
            <strong><%= d.mellomnavn %></strong> 
            <strong><%= d.etternavn %></strong> 
            <strong><%= d.alder %></strong> 
            <strong><%= d.email %></strong> 
          </li>
        <% }); %>
    </ul>
    
    <div class="container">
      <div class="header">
        <h1>Welcome to HelpDesk</h1>
      </div>
      
      <div class="nav">
        <ul>
          <li><a href="home_site">Home</a></li>
          <li><a href="about_us">About us</a></li>
          <li><a href="contact_agent">Contact Agent</a></li>
        </ul>
      </div>
      
      <div class="content">
        <h2>About us</h2>
        <p>Contact Agent.</p>
      </div>
      
      <div class="footer">
        <p>&copy; Agent.</p>
      </div>
    </div>

    <div id="holder">
      WELCOME
    </div>
        


    <button><a href="/log_out">Logout</a></button>
    <button><a href="/change_account">Change account?</a></button>
 
    <script src="klient.js"></script>
</body>
</html>